<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav_graph"
    app:startDestination="@+id/mapFragment">


    <fragment
        android:id="@+id/mapFragment"
        android:name="com.mapcok.ui.map.MapFragment"
        tools:layout="@layout/fragment_map" />

    <fragment
        android:id="@+id/userListFragment"
        android:name="com.mapcok.ui.userlist.UserListFragment"
        tools:layout="@layout/fragment_user_list" >

    </fragment>

    <fragment
        android:id="@+id/myPageFragment"
        android:name="com.mapcok.ui.mypage.MyPageFragment"
        tools:layout="@layout/fragment_my_page" >
        <action
          android:id="@+id/action_myPageFragment_to_myPhotoFragment"
          app:destination="@id/myPhotoFragment" />
    </fragment>

    <fragment
        android:id="@+id/myPhotoFragment"
        android:name="com.mapcok.ui.myphoto.MyPhotoFragment"
        tools:layout="@layout/fragment_my_photo"/>

</navigation>